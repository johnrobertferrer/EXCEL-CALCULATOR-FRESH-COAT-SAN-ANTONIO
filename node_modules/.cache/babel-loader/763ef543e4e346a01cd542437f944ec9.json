,KAAA,CAAA,UAAA,CAAA;AACA,UAAA,EAAA,GAAA,UAAA,IAAA,QAAA,CAAA,UAAA,CAAA,CAAA,CAAA,CAAA,GAAA,QAAA,GAAA,CAAA,CAAA;;AAEA,UAAA,QAAA,KAAA,CAAA,IAAA,EAAA,EAAA;AACA,QAAA,QAAA,CAAA,cAAA,CAAA,EAAA,EAAA,KAAA;AACA;AACA;AA5BA,GAXA;AA0CA,EAAA,QAAA,EAAA;AACA,IAAA,YAAA,EAAA;AACA,MAAA,GAAA,EAAA,eAAA;AACA;AACA,YAAA,KAAA,GAAA,KAAA,KAAA;;AAEA,YAAA,KAAA,aAAA,EAAA;AACA;AACA,iBAAA,KAAA,CAAA,QAAA,EAAA;AACA,SAHA,MAGA;AACA,cAAA,CAAA,QAAA,CAAA,KAAA,CAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,CADA,CACA;AACA;;AAEA,cAAA,MAAA,GAAA,KAAA,CAAA,OAAA,CAAA,CAAA,EAAA,OAAA,CAAA,8BAAA,EAAA,KAAA,CAAA;;AAEA,kBAAA,KAAA,IAAA;AACA,iBAAA,QAAA;AACA,cAAA,MAAA,GAAA,OAAA,KAAA,CAAA,OAAA,CAAA,CAAA,EAAA,OAAA,CAAA,8BAAA,EAAA,KAAA,CAAA;AACA;;AACA,iBAAA,SAAA;AACA,cAAA,MAAA,GAAA,KAAA,CAAA,OAAA,CAAA,CAAA,IAAA,GAAA;AACA;;AACA,iBAAA,oBAAA;AACA;AACA;AACA;AACA,cAAA,MAAA,GAAA,QAAA,KAAA,CAAA,OAAA,CAAA,CAAA,EAAA,OAAA,CAAA,8BAAA,EAAA,KAAA,CAAA,GAAA,GAAA;AACA;AAZA,WAPA,CAqBA;;;AACA,iBAAA,MAAA;AACA;AACA,OAhCA;AAiCA,MAAA,GAAA,EAAA,aAAA,aAAA,EAAA;AACA;AACA,YAAA,QAAA,GAAA,UAAA,CAAA,aAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA,CAAA,CAAA,CAFA,CAEA;AACA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,EAAA;AACA,UAAA,QAAA,GAAA,CAAA;AACA,SANA,CAOA;AACA;;;AACA,aAAA,KAAA,CAAA,OAAA,EAAA,QAAA;AACA;AA3CA,KADA;AA+CA,IAAA,WAAA,EAAA,uBAAA;AACA,UAAA,MAAA,GAAA,KAAA,OAAA,KAAA,SAAA,GAAA,EAAA,GAAA,KAAA,OAAA;AAEA,aAAA,MAAA;AACA;AAnDA;AA1CA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <b-form-input type=\"text\" v-model=\"displayValue\" @blur=\"isInputActive = false\" @focus=\"isInputActive = true\" :disabled=\"disabled\" :class=\"customClass\" v-on:keyup.enter=\"focus($event)\"/>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'CustomInput',\r\n\r\n    props: [\"value\", \"disabled\", \"alias\", \"type\", \"classes\"],\r\n\r\n    data() {\r\n        return {\r\n            isInputActive: false\r\n        }\r\n    },\r\n\r\n    methods: {\r\n        focus(event) {\r\n            let interval = 0;\r\n\r\n            switch(this.alias) {\r\n                case 'average_hourly_rate': \r\n                case 'rounded_labor_days': \r\n                    interval = 1;\r\n                    break;\r\n                case 'burden_percent': \r\n                case 'labor_target_percent': \r\n                    interval = 2;\r\n                    break;\r\n                case 'epic_work_order_hours': \r\n                    interval = 3;\r\n                    break;\r\n                case 'billable_hours_day': \r\n                    interval = 4;\r\n                    break;\r\n            }\r\n\r\n            let prependKey = '__BVID__';\r\n            let splittedId = event.target.id.split(prependKey);\r\n            let id = prependKey + (parseInt(splittedId[1]) + (interval * 2));\r\n\r\n            if(interval !== 0 && id) {\r\n                document.getElementById(id).focus();\r\n            }\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        displayValue: {\r\n            get: function() {\r\n                // Note: we cannot set this.value as it is a \"prop\". It needs to be passed to parent component\r\n                let value = this.value;\r\n\r\n                if (this.isInputActive) {\r\n                    // Cursor is inside the input field. unformat display value for user\r\n                    return value.toString();\r\n                } else {\r\n                    if(!isFinite(value)) {\r\n                        value = 0; // eslint-disable-line\r\n                    } \r\n\r\n                    let format = value.toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\");\r\n\r\n                    switch(this.type) {\r\n                        case 'dollar':\r\n                            format = \"$ \" + value.toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\");\r\n                            break;\r\n                        case 'percent': \r\n                            format = value.toFixed(2) + '%';\r\n                            break;\r\n                        case 'dollar_parenthesis':\r\n                            // format = value < 0\r\n                            //             ? \"- $ (\" + (value*-1).toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\") + ')'\r\n                            //             : \"$ (\" + value.toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\") + ')';\r\n                            format = \"$ (\" + value.toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\") + ')';\r\n                            break;\r\n                    }\r\n                    // User is not modifying now. Format display value for user interface\r\n                    return format;\r\n                }\r\n            },\r\n            set: function(modifiedValue) {\r\n                // Recalculate value after ignoring \"$\" and \",\" in user input\r\n                let newValue = parseFloat(modifiedValue.replace(/[^\\d\\.]/g, \"\")) // eslint-disable-line\r\n                // Ensure that it is not NaN\r\n                if (isNaN(newValue)) {\r\n                    newValue = 0\r\n                }\r\n                // Note: we cannot set this.value as it is a \"prop\". It needs to be passed to parent component\r\n                // $emit the event so that parent component gets it\r\n                this.$emit('input', newValue);\r\n            }\r\n        },\r\n\r\n        customClass: function() {\r\n            let custom = this.classes === undefined ? '' : this.classes;\r\n\r\n            return custom;\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n    .noBoxShadow {\r\n        border-color: inherit !important;\r\n        -webkit-box-shadow: none !important;\r\n        box-shadow: none !important;\r\n    }\r\n\r\n    .bg-gray-300 {\r\n        background: #ced4da;\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}