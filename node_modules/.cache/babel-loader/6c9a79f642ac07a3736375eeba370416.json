AAA,CAFA,CAGA;;AACA,YAAA,KAAA,CAAA,QAAA,CAAA,EAAA;AACA,UAAA,QAAA,GAAA,CAAA;AACA,SANA,CAOA;AACA;;;AACA,aAAA,KAAA,CAAA,OAAA,EAAA,QAAA;AACA;AApBA;AADA;AAXA,CAAA","sourcesContent":["<template>\r\n    <div>\r\n        <input type=\"text\" v-model=\"displayValue\" @blur=\"isInputActive = false\" @focus=\"isInputActive = true\"/>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'DollarCurrencyInput',\r\n\r\n    props: [\"value\"],\r\n\r\n    data() {\r\n        return {\r\n            isInputActive: false\r\n        }\r\n    },\r\n\r\n    computed: {\r\n        displayValue: {\r\n            get: function() {\r\n                if (this.isInputActive) {\r\n                    // Cursor is inside the input field. unformat display value for user\r\n                    return this.value.toString()\r\n                } else {\r\n                    // User is not modifying now. Format display value for user interface\r\n                    return \"$ \" + this.value.toFixed(2).replace(/(\\d)(?=(\\d{3})+(?:\\.\\d+)?$)/g, \"$1,\")\r\n                }\r\n            },\r\n            set: function(modifiedValue) {\r\n                // Recalculate value after ignoring \"$\" and \",\" in user input\r\n                let newValue = parseFloat(modifiedValue.replace(/[^\\d\\.]/g, \"\"))\r\n                // Ensure that it is not NaN\r\n                if (isNaN(newValue)) {\r\n                    newValue = 0\r\n                }\r\n                // Note: we cannot set this.value as it is a \"prop\". It needs to be passed to parent component\r\n                // $emit the event so that parent component gets it\r\n                this.$emit('input', newValue)\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components"}]}